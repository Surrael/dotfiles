      "headset": "ÔÄ• ",
      "phone": "ÔÇï ",
      "portable": "ÔÇï ",
      "car": "ÔÜπ  ",
      "default": ["ÔÄ¶ ", "ÔÄß ", "ÔÄ® "]
    }
  },
  "bluetooth": {
    "format-off": "<span size='13000' foreground='#585b70'>Ôäî </span>",
    "format-connected": "<span size='13000' foreground='#89b4fa'>Ôäî </span>{device_alias}",
    "tooltip-format": "Bluetooth: {status}\nDevice: {device_alias}\nMAC: {device_address}",
